<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" style="-fx-background-color: #383838;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ViewPKG.LoginPageController">
    <children>
        <Label alignment="CENTER" layoutX="207.0" layoutY="111.0" text="Password" textFill="#8888f6">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="passwordField" layoutX="14.0" layoutY="108.0" style="-fx-background-color: #686868;" />
        <TextField fx:id="usernameField" layoutX="14.0" layoutY="66.0" style="-fx-background-color: #686868;" />
        <Label alignment="CENTER" layoutX="21.0" layoutY="14.0" prefHeight="27.0" prefWidth="257.0" text="Please Enter Your Credentials" textFill="#8888f6">
            <font>
                <Font size="18.0" />
            </font></Label>
        <Label alignment="CENTER" layoutX="196.0" layoutY="69.0" prefHeight="20.0" prefWidth="81.0" text="Username" textFill="#8888f6">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Button fx:id="submitButton" layoutX="169.0" layoutY="160.0" mnemonicParsing="false" onAction="#submit" style="-fx-background-color: #8888f6;" text="Submit" textFill="#404040" />
        <Label fx:id="errorField" alignment="CENTER" layoutX="4.0" layoutY="159.0" prefHeight="26.0" prefWidth="160.0" style="-fx-background-color: #383838;" textFill="RED">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
      <Button fx:id="backButton" layoutX="231.0" layoutY="160.0" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="54.0" style="-fx-background-color: #8888f6;" text="Back" textFill="#404040">
         <font>
            <Font name="Calibri" size="13.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
